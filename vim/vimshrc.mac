alias ls='ls -vGF $$args'
alias ll='ls -vGF -l $$args'
alias l.='ls -vGF -d .*'
alias vp='vimpager $$args'
alias psl='ps -arcwwwxo "pid command %cpu %mem" | grep -v grep | head -13'
alias prove='prove --rc $H/.proverc $$args'
alias h='cd $H'
alias m='cd $M'
alias hub='git'
alias rfd='rspec --format documentation'
export PATH=/usr/local/opt/rbenv/bin:/usr/local/opt/rbenv/shims:/usr/local/opt/plenv/bin:/usr/local/opt/plenv/shims:/usr/local/opt/pyenv/bin:/usr/local/opt/pyenv/shims:$PATH
