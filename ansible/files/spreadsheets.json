{
    "title": "Custom shortcut for Google Spreadsheets",
    "rules": [
        {
            "description": "Use left_control+left_option+xc to cycle through tabs.",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "x",
                        "modifiers": {
                            "mandatory": [
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "page_up",
                            "modifiers": [
                                "fn",
                                "left_command",
                                "left_shift"
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "c",
                        "modifiers": {
                            "mandatory": [
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "page_down",
                            "modifiers": [
                                "fn",
                                "left_command",
                                "left_shift"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
