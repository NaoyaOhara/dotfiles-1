{
  "title": "[delphinus] Custom shortcut for return_or_enter",
  "rules": [
    {
      "description": "[delphinus] Change return_or_enter to fn. (Post return_or_enter if pressed alone)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "fn"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "return_or_enter"
            }
          ]
        }
      ]
    }
  ]
}
