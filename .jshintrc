/* vim:se ft=javascript: */
/* http://qiita.com/xorphitus/items/f26ec8fbe17c317d3994 */
{
    /* Enforcing options */
    "bitwise": true,
    "camelcase": false,
    "curly": true,
    "eqeqeq": true,
    "es3": false,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": false,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": false,
    "undef": true,
    "unused": true,
    "strict": false,
    "trailing": true,
    "maxparams": false,
    "maxdepth": false,
    "maxstatements": false,
    "maxcomplexity": false,
    "maxlen": false,
    "validthis": true,

    /* Environments */
    "browser": true,
    "couch": false,
    "devel": true,
    "dojo": false,
    "jquery": true,
    "mootools": false,
    "node": false,
    "nonstandard": false,
    "phantom": false,
    "prototypejs": false,
    "rhino": false,
    "worker": false,
    "wsh": false,
    "yui": false,

    "predef": [
        "_",
        "Backbone",
        "AppWidgets",
        "ModerationDetails",
        "require",
        "module"
    ]
}
