{
	"common": {
		"term_truecolor": false,
		"dividers": {
			"left": {
				"hard": "⮀ ",
				"soft": "⮁ "
			},
			"right": {
				"hard": " ⮂",
				"soft": " ⮃"
			}
		},
		"spaces": 1,
		"paths": [
			"~/.config/powerline"
		]
	},
	"ext": {
		"ipython": {
			"colorscheme": "default",
			"theme": "default"
		},
		"shell": {
			"colorscheme": "solarized",
			"theme": "custom"
		},
		"tmux": {
			"colorscheme": "default",
			"theme": "default"
		},
		"vim": {
			"colorscheme": "custom",
			"theme": "custom",
			"local_themes": {
				"cmdwin": "cmdwin",
				"help": "help"
			}
		},
		"wm": {
			"colorscheme": "default",
			"theme": "default"
		}
	}
}
