{
	"default_module": "powerline.segments.common",
	"segment_data": {
		"uptime": {
			"before": "⇑  "
		},
		"external_ip": {
			"before": "ⓦ  "
		},
		"date": {
			"before": "⌚ "
		},
		"weather": {
			"args": {
				"location_query": "tokyo, japan"
			}
		},
		"email_imap_alert": {
			"before": "✉ ",
			"args": {
				"username": "",
				"password": ""
			}
		},
		"cpu_load_percent_gradient": {
			"args": {
				"format": "CPU {0}%"
			}
		},
		"system_load": {
			"args": {
				"threshold_good": 0.25,
				"threshold_bad": 0.5
			}
		}
	},
	"segments": {
		"right": [
			{
				"name": "now_playing",
				"module": "ext.tmux.segments.custom",
				"args": {
					"player": "lastfm",
					"format": "{state_symbol} {artist} - {title} ({elapsed})"
				},
				"priority": 80
			},
			{
				"name": "network_load",
				"priority": 50
			},
			{
				"name": "cpu_load_percent_gradient",
				"module": "ext.tmux.segments.custom",
				"priority": 50
			},
			{
				"name": "system_load",
				"priority": 50
			},
			{
				"name": "date",
				"priority": 80
			},
			{
				"name": "date",
				"args": {
					"format": "%H:%M",
					"istime": true
				},
				"priority": 80
			},
			{
				"name": "hostname",
				"priority": 80
			}
		]
	},
	"modeline": " vim:se ft=javascript noet: "
}
