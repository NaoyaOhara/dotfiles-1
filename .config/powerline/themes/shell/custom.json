{
	"default_module": "powerline.segments.common",
	"segments": {
		"left": [
			{
				"name": "hostname",
				"before": " ",
				"args": {
					"only_if_ssh": true
				}
			},
			{
				"name": "user"
			},
			{
				"name": "virtualenv",
				"before": "ⓔ  "
			},
			{
				"name": "branch",
				"before": " "
			},
			{
				"name": "cwd",
				"args": {
					"dir_limit_depth": 3
				},
				"divider_highlight_group": "cwd:divider"
			},
			{
				"type": "string",
				"contents": "\n\r",
				"draw_divider": false,
				"highlight_group": ["none"]
			}
		]
	}
}
