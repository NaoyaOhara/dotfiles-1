{
	"default_module": "powerline.segments.common",
	"segment_data": {
		"branch": {
			"before": " ",
			"status_colors": true
		},
		"cwd": {
			"args": {
				"dir_limit_depth": 5,
				"use_path_separator": false,
				"dir_shorten_len": 1
			}
		},
		"hostname": {
			"before": " ",
			"args": {
				"only_if_ssh": true
			}
		},
		"mode": {
			"vicmd": "N",
			"viins": "I"
		},
		"virtualenv": {
			"before": "ⓔ  "
		}
	},
	"segments": {
		"left": [
			{
				"module": "powerline.segments.shell",
				"name": "mode"
			},
			{
				"name": "hostname"
			},
			{
				"name": "cwd"
			},
			{
				"module": "powerline.segments.shell",
				"name": "jobnum"
			}
		],
		"right": [
			{
				"module": "powerline.segments.shell",
				"name": "last_pipe_status"
			},
			{
				"name": "branch"
			},
			{
				"name": "virtualenv"
			}
		]
	}
}
