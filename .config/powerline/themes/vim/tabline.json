{
	"default_module": "powerline.segments.vim",
	"segments": {
		"left": [
			{
				"function": "file_name"
			},
			{
				"function": "file_size",
				"before": "size: "
			},
			{
				"function": "ext.vim.segments.custom.file_mtime",
				"before": "mtime: "
			},
			{
				"type": "segment_list",
				"function": "powerline.listers.vim.tablister",
				"exclude_function": "single_tab",
				"segments": [
					{
						"function": "tabnr",
						"after": " ",
						"priority": 5
					},
					{
						"function": "file_directory",
						"priority": 40
					},
					{
						"function": "file_name",
						"args": {
							"display_no_file": true
						},
						"priority": 10
					},
					{
						"function": "tab_modified_indicator",
						"priority": 5
					}
				]
			},
			{
				"type": "string",
				"highlight_group": ["background"],
				"draw_soft_divider": false,
				"draw_hard_divider": false,
				"width": "auto"
			}
		],
		"right": [
			{
				"function": "powerline.segments.common.time.date"
			},
			{
				"function": "powerline.segments.common.time.date",
				"args": {
					"format": "%H:%M",
					"istime": true
				}
			},
			{
				"type": "string",
				"contents": "Tabs",
				"name": "many_tabs",
				"highlight_group": ["many_tabs"],
				"exclude_function": "single_tab"
			}
		]
	}
}
