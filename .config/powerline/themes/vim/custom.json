				"exclude_modes": ["nc"],
				"priority": 70
			},
			{
				"name": "line_percent",
				"priority": 90,
				"width": 4,
				"align": "r"
			},
			{
				"type": "string",
				"name": "line_current_symbol",
				"highlight_group": ["line_current_symbol", "line_current"],
				"priority": 20
			},
			{
				"name": "line_current",
				"draw_soft_divider": false,
				"width": 3,
				"align": "r",
				"priority": 20
			},
			{
				"name": "virtcol_current",
				"draw_soft_divider": false,
				"priority": 90,
				"before": ":",
				"align": "l"
			},
			{
				"name": "col_current",
				"draw_soft_divider": false,
				"priority": 90,
				"before": "-",
				"align": "l"
			}
		]
