{
	"segment_data": {
		"powerline.segments.vim.modified_indicator": {
			"args": { "text": "âš¡" }
		},
		"line_percent": {
			"args": { "gradient": true },
			"after": "%"
		},
		"col_current_virt": {
			"args": { "gradient": true }
		}
	},
	"segments": {
		"left": [
			{
				"name": "mode",
				"exclude_modes": ["nc"]
			},
			{
				"name": "visual_range",
				"include_modes": ["v", "V", "^V", "s", "S", "^S"],
				"priority": 20
			},
			{
				"name": "paste_indicator",
				"exclude_modes": ["nc"],
				"priority": 20
			},
			{
				"name": "branch",
				"exclude_modes": ["nc"],
				"priority": 20
			},
			{
				"name": "readonly_indicator",
				"draw_soft_divider": false,
				"after": " ",
				"priority": 20
			},
			{
				"name": "file_directory",
				"priority": 10,
				"draw_soft_divider": false
			},
			{
				"name": "file_name",
				"draw_soft_divider": false
			},
			{
				"name": "file_vcs_status",
				"before": " ",
				"draw_soft_divider": false,
				"priority": 20
			},
			{
				"name": "modified_indicator",
				"before": " "
			},
			{
				"exclude_modes": ["i", "R", "Rv"],
				"name": "trailing_whitespace",
				"display": false,
				"priority": 60
			},
			{
				"exclude_modes": ["nc"],
				"module": "powerline.segments.vim.plugin.syntastic",
				"name": "syntastic",
				"priority": 50
			},
			{
				"exclude_modes": ["nc"],
				"module": "powerline.segments.vim.plugin.tagbar",
				"name": "current_tag",
				"draw_soft_divider": false,
				"priority": 50
			},
			{
				"type": "string",
				"highlight_group": ["background"],
				"draw_soft_divider": false,
				"draw_hard_divider": false,
				"width": "auto"
			}
		],
		"right": [
			{
				"name": "file_format",
				"draw_soft_divider": false,
				"exclude_modes": ["nc"],
				"priority": 90
			},
			{
				"name": "file_encoding",
				"exclude_modes": ["nc"],
				"priority": 90
			},
			{
				"name": "file_type",
				"exclude_modes": ["nc"],
				"priority": 80
			},
			{
				"name": "get_char_code",
