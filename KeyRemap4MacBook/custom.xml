<?xml version="1.0"?>
<root>
    <appdef>
        <appname>MACVIM</appname>
        <equal>org.vim.MacVim</equal>
    </appdef>
    <appdef>
        <appname>CONTEXTS</appname>
        <equal>com.contextsformac.Contexts</equal>
    </appdef>
    <item>
        <name>Custom Keymaps</name>
        <list>
            <item>
                <name>C-TAB to C-Z, TAB (change window)</name>
                <identifier>remap.app_term_screen_tab</identifier>
                <only>TERMINAL</only>
                <autogen>--KeyToKey-- KeyCode::TAB, ModifierFlag::CONTROL_L, KeyCode::Z, ModifierFlag::CONTROL_L, KeyCode::TAB</autogen>
                <autogen>--KeyToKey-- KeyCode::TAB, ModifierFlag::CONTROL_R, KeyCode::Z, ModifierFlag::CONTROL_L, KeyCode::TAB</autogen>
            </item>
            <item>
                <name>C-TAB to C-Z, U (change window for dvtm)</name>
                <identifier>remap.app_term_dvtm_tab</identifier>
                <only>TERMINAL</only>
                <autogen>--KeyToKey-- KeyCode::TAB, ModifierFlag::CONTROL_L, KeyCode::Z, ModifierFlag::CONTROL_L, KeyCode::U</autogen>
            </item>
            <item>
